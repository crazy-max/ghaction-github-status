{
  "name": "github-status",
  "description": "GitHub Action to check GitHub Status in your workflow",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc && ncc build",
    "test": "jest --coverage",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "cleanup-paths": "removeNPMAbsolutePaths ./ --force --fields _where _args",
    "pre-checkin": "npm run format && npm run cleanup-paths && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crazy-max/ghaction-github-status.git"
  },
  "keywords": [
    "actions",
    "github",
    "status",
    "monitoring"
  ],
  "author": "CrazyMax",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.4",
    "@actions/http-client": "^1.0.8",
    "chalk": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.9",
    "@zeit/ncc": "^0.22.2",
    "jest": "^25.5.4",
    "jest-circus": "^26.0.1",
    "jest-runtime": "^26.0.1",
    "prettier": "^2.0.4",
    "removeNPMAbsolutePaths": "^2.0.0",
    "ts-jest": "^25.5.1",
    "typescript": "^3.9.2"
  }
}
